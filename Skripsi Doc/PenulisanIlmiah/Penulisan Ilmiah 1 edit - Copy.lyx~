	filename D:/ADIT's/Others/Doc/SKRIPSI DOC/Images/erd.png
	scale 70

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout

\lang english
Rancangan ERD
\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard

\lang english
Penjelasan : 
\end_layout

\begin_layout Itemize

\lang english
Entity Traveller / user berisi data-data user seperti nama, email, password,
 hubungan, foto, pekerjaan, agama, hobi, kontak dan bio.
 
\end_layout

\begin_layout Itemize

\lang english
Entity Rumah Singgah digunakan untuk menampung data-data pencari atau mencari
 rumah singgah seperti user_id, lokasi, kontak dan deskripsi.
\end_layout

\begin_layout Itemize

\lang english
Entity Info Wisata digunakan untuk menampung data-data informasi wisata
 yang dibuat oleh user seperti user_id, kategori, judul, foto dan deskripsi.
\end_layout

\begin_layout Itemize

\lang english
Entity Barengan digunakan untuk menampung data-data user yang mencari barengan
 saat akan berwisata seperti user_id, asal, tujuan, desripsi, kontak, tanggal
 buat, tanggal berangkat dan tanggal pulang.
\end_layout

\begin_layout Itemize

\lang english
Entity Diskusi / Home digunakan untuk menampung data - data diskusi user
 seperti user_id, tanggal dan post.
\end_layout

\begin_layout Itemize

\lang english
Enity Galeri digunakan untuk menampung data-data foto yang user simpan seperti
 user_id, images dan caption
\end_layout

\end_deeper
