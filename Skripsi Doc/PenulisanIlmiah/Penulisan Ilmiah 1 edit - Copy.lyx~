\begin_layout Standard

\lang english
Penjelasan : 
\end_layout

\begin_layout Standard

\lang english
Dalam pembuatan rancangan relasi database tersebut mempunyai 11 tabel yaitu
\end_layout

\begin_layout Enumerate
Tabel users (untuk menampung data - data user)
\end_layout

\begin_layout Enumerate
Tabel galeries (untuk menampung data - data galeri user)
\end_layout

\begin_layout Enumerate
Tabel posts (untuk menampung data - data diskusi user)
\end_layout

\begin_layout Enumerate
Tabel post_comments (untuk menampung data - data komentar diskusi user)
\end_layout

\begin_layout Enumerate
Tabel barengans (untuk menampung data - data user yang akan mencari barengan)
\end_layout

\begin_layout Enumerate
Tabel barengan_comments (untuk menapung data - data komentar cari barengan)
\end_layout

\begin_layout Enumerate
Tabel singgahs (untuk menampung data - data user yang akan mencari atau
 menyediakan rumah singgah)
\end_layout

\begin_layout Enumerate
Tabel singgah_comments (untuk menampung data - data komentar rumah singgah)
\end_layout

\begin_layout Enumerate
Tabel infos (untuk menampung data - data user berbagi info wisata)
\end_layout

\begin_layout Enumerate
Tabel info_comments (untuk menampung data - data komentar info wisata)
\end_layout

\begin_layout Enumerate
Tabel categories (untuk menampung data - data kategori info wisata)
\end_layout

\begin_layout Standard

\lang english
Dapat dilihat bahwa relasi antara tabel adalah field user_id dan id dari
 masing - masing table tersebut.
\end_layout

\end_deeper
